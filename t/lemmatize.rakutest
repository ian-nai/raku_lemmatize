use Test;      # a Standard module included with Rakudo 
use lib '.';
use lemmatize; 

 
plan 4;
 
# .... tests 
my $test_sent = "Having a test.";


my %lemma_hash = construct_hash('resources/lemmas_test.csv');
is %lemma_hash<have>.[0][0], 'had';

